{
    "name": "rusbeldoor/yii2-general",
    "type": "library",
    "require": {
        "php": ">=8.0.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap5": "*",
        "thomaspark/bootswatch": "^5",
        "fortawesome/font-awesome": "^5.14.0",
        "kartik-v/yii2-bootstrap5-dropdown": "@dev",
        "kartik-v/yii2-widget-activeform": "@dev",
        "kartik-v/yii2-field-range": "@dev",
        "kartik-v/yii2-date-range": "@dev",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-sortable-input": "@dev"
    },
    "config": {
        "platform": {"php": "8.0"},
        "process-timeout": 1800,
        "fxp-asset": {"enabled": false}
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ],
    "autoload": {
        "psr-4": {
            "rusbeldoor\\yii2General\\": ""
        }
    }
}
